<UserControl x:Class="ADCMonitor.View.Monitor.WaveformPropertyGrid_UserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
             xmlns:local="clr-namespace:ADCMonitor.View.Monitor"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <telerik:RadPropertyGrid x:Name="propertyGrid" 
                                 Item="{Binding ADCMonitorVM, Source={StaticResource Locator}}"
                                 AutoGeneratingPropertyDefinition="propertyGrid_AutoGeneratingPropertyDefinition">
            
        </telerik:RadPropertyGrid>
    </Grid>
</UserControl>
